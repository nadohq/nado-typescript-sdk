{
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "exclude": [
    "**/node_modules/**/*",
    "**/dist/**/*"
  ],
  "out": "apps/docs",
  "internalModule": "Internal",
  "sort": [
    "visibility",
    "source-order"
  ],
  "plugin": [
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-extras"
  ]
}